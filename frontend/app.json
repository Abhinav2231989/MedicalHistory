{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",

    "jsEngine": "hermes",

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abhinavshaurya.medicalhistory",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "package": "com.abhinavshaurya.medicalhistory",
      "edgeToEdgeEnabled": true,
      "newArchEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "EXPO_PUBLIC_BACKEND_URL": "http://192.168.1.7:8000",
      "eas": {
        "projectId": "4f8d3f17-4fdf-4e7e-a105-4b2de3613971"
      }
    }
  }
}
